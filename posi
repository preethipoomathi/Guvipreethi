package wipro4;

import java.util.*;
import java.lang.*;
import java.io.*;

 
class posi
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int a=sc.nextInt();
 
if(a==0)
{
System.out.println("Zero");
}
else if(a>0)
{
System.out.println("Positive");
}
else
{
System.out.println("Negative");
}
}
}
